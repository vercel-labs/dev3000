{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "BUILD_DIR=$PWD && cd .. && pnpm install && cd \"$BUILD_DIR\" && NODE_PATH=../node_modules node ../node_modules/next/dist/bin/next build --turbopack",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "installCommand": "echo 'Install handled in buildCommand'",
  "framework": null,
  "env": {
    "LOG_FILE_PATH": "/tmp/d3k/logs",
    "SCREENSHOT_DIR": "/tmp/d3k/screenshots"
  },
  "rewrites": [
    {
      "source": "/mcp/:path*",
      "destination": "/api/mcp/:path*"
    }
  ]
}
