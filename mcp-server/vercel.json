{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd .. && cd mcp-server && node ../node_modules/next/dist/bin/next build --turbopack",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "installCommand": "cd .. && pnpm install",
  "framework": null,
  "env": {
    "LOG_FILE_PATH": "/tmp/d3k/logs",
    "SCREENSHOT_DIR": "/tmp/d3k/screenshots"
  },
  "rewrites": [
    {
      "source": "/mcp/:path*",
      "destination": "/api/mcp/:path*"
    }
  ]
}
