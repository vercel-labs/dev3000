{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "../node_modules/.bin/next build --turbopack",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "installCommand": "SUBDIR=$PWD && cd .. && pnpm install && cd \"$SUBDIR\" && ln -sf ../node_modules node_modules",
  "framework": null,
  "env": {
    "LOG_FILE_PATH": "/tmp/d3k/logs",
    "SCREENSHOT_DIR": "/tmp/d3k/screenshots"
  },
  "rewrites": [
    {
      "source": "/mcp/:path*",
      "destination": "/api/mcp/:path*"
    }
  ]
}
